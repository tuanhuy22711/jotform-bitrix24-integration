# Server Configuration
PORT=3000
NODE_ENV=development

# Jotform Configuration
JOTFORM_API_KEY=your_jotform_api_key_here
JOTFORM_FORM_ID=your_form_id_here
JOTFORM_API_URL=https://api.jotform.com

# Bitrix24 Configuration
BITRIX24_WEBHOOK_URL=your_bitrix24_webhook_url_here
BITRIX24_REST_URL=your_bitrix24_webhook_url_here
BITRIX24_DOMAIN=your_bitrix24_domain.bitrix24.com
BITRIX24_ACCESS_TOKEN=your_bitrix24_access_token_here

# Security
WEBHOOK_SECRET=your_webhook_secret_here
API_RATE_LIMIT=100

# Logging
LOG_LEVEL=info
LOG_FILE=logs/app.log
